{
  "name": "react-rxbuilder",
  "version": "0.6.1",
  "description": "Componentized Rxjs Observable",
  "private": false,
  "main": "./dist/umd/react-rxbuilder.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "start": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=development ts-node config/webpack/dev-server.ts",
    "esm": "tsc --project tsconfig.esm.json",
    "umd": "cross-env TS_NODE_PROJECT=config/webpack/tsconfig.json NODE_ENV=production webpack -c config/webpack/prod.config.ts",
    "build": "npm run umd && npm run esm"
  },
  "keywords": [
    "react-rxbuilder",
    "react",
    "rxjs",
    "rx",
    "react builder",
    "react rxjs",
    "RxBuilder",
    "PromiseBuilder",
    "Ajanuw"
  ],
  "author": "Ajanuw",
  "license": "MIT",
  "homepage": "https://github.com/januwA/react-rxbuilder",
  "repository": {
    "type": "git",
    "url": "https://github.com/januwA/react-rxbuilder.git"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/webpack": "^5.28.0",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.3.2",
    "json5": "^2.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rxjs": "^7.2.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  }
}
